<?xml version="1.0"?>
<root name="kinect_properties" xmlns:xacro="http://ros.org/wiki/xacro">
    <!--
        Translation and rotation of camera from its base link
     -->
    <xacro:property name="cam_px" value="0.10"/>
    <xacro:property name="cam_py" value="0.0"/>
    <xacro:property name="cam_pz" value="1.10"/>
    <xacro:property name="cam_or" value="0"/>
    <xacro:property name="cam_op" value="1.50"/>
    <xacro:property name="cam_oy" value="0"/>
</root>
